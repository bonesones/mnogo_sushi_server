{
  "version": 2,
  "builds": [
    {
      "src": "/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "./index.js",
      "methods": [
        "GET",
        "POST",
        "PUT",
        "UPDATE",
        "DELETE",
        "OPTIONS",
        "PATCH"
      ],
      "headers": {
        "Access-Control-Allow-Origin": "https://mnogo-sushi-phi.vercel.app",
        "Access-Control-Allow-Credentials": "true"
      }
    }
  ]
}