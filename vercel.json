{
  "version": 2,
  "builds": [
    {
      "src": "/index.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/api/:path(.*)",
      "destination": "https://mnogo-sushi-server.vercel.app/api/:path"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}